generator client {
  provider   = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// Masukkan satu model sederhana untuk testing
model Laporan {
  id        Int      @id @default(autoincrement())
  judul     String
  createdAt DateTime @default(now())
}